    "map_tweaks":{
        // Outskirts (MAP02)
        "MAP02":{
            "sidedefs":{
                // Mark the switch we're changing to require the blue key.
                "1329":{"middle":"DOORBLU"},
                "1330":{"middle":"DOORBLU"}
            },
            "linedefs":{
                // Remove a platform raise that blocks you from returning permanently
                "5":{"special":0},

                // Require blue key to advance the map early on, as anti-softlock
                "845":{"special":133}
            },
            "sectors":{
                // Open the backup key from the start.
                "393":{"ceiling":44},

                // Slightly raise the starting room so we can re-enter it.
                "121":{"floor":-32,"ceiling":32},
                "122":{"floor":-32,"ceiling":32},
                "123":{"floor":-32,"ceiling":-32},
                "233":{"floor":-20,"ceiling":28},
                "234":{"floor":-32,"ceiling":40},
                "235":{"floor":-20,"ceiling":28},
                "265":{"floor":-24}
            },
            "things":{
                // Move the backup blue key item out a bit, to make it easier to grab
                "433":{"x":-1440,"y":-2026}
            }
        },

        // Depot (MAP05)
        "MAP05":{
            // Move the hub out of the way of a door
            "hub":{"x":-224,"y":-184}
        },

        // En Route (MAP07)
        "MAP07":{
            // Remove normal Doom II MAP07 hacks, as this map doesn't use them
            // (this allows enemy rando freely on this map)
            "metadata":{"behaves_as": "NORMAL"},

            // Move the hub out of the way of the exit
            "hub":{"x":-112,"y":-2064}
        },

        // Entryway (MAP10)
        "MAP10":{
            "sectors":{
                // Require the red key to progress past the start
                "9":{"ceiling":0},
                "10":{"ceiling":0},
                "11":{"ceiling":0},
                "12":{"ceiling":0},

                // Pre-open the switch that we're making red key required
                "357":{"ceiling":176}
            },
            "linedefs":{
                // Remove the now redundant trigger to open the switch at the start
                "1506":{"special":0},

                // Make that switch red key required
                "1507":{"special":134},

                // Do *not* require the blue key to get out of the central star
                // We cannot make this a two-way keydoor because the linedefs on the other side are oriented wrong
                "940":{"special":31},
                "944":{"special":31}
            },
            "sidedefs":{
                // Mark the switch we're changing to require the red key.
                "2284":{"middle":"DOORRED2"},
                "2286":{"middle":"DOORRED2"},
                "2290":{"middle":"DOORRED2","x":8},
                "2291":{"middle":"DOORRED2","x":8},

                // Remove the blue key appearance from the bars at the central star
                "1419":{"upper":"BORDER2"},
                "1427":{"upper":"BORDER2"}
            }
        },

        // Ruins (MAP13)
        "MAP13":{
            "linedefs":{
                // Allow opening the bar that locks you in the vile trap, from behind
                "1520":{"special":118}
            }
        },

        // Subsidiary Power (MAP15)
        "MAP15":{
            // Open the door back from the eastern room ASAP to prevent multiple possible softlocks
            "linedefs":{
                "508":{"special":2,"tag":690},
                "576":{"special":74}
            },
            "sectors":{
                "80":{"tag":690},
                "118":{"tag":690}
            }
        },

        // Living Quarters (MAP16)
        "MAP16":{
            "sectors":{
                // Tag rails of blue key door room so they can act as a lift
                "59":{"tag":690},
                "134":{"tag":690}
            },
            "linedefs":{
                // Give lift special to the rails of the blue key door room (anti-softlock)
                "644":{"special":62,"tag":690},
                "645":{"special":62,"tag":690}
            }
        },

        // Promenade (MAP17)
        "MAP17":{
            "linedefs":{
                // Gauntlet Hallway hackery - remove the door close triggers.
                "2414": {"special": 0},
                "2415": {"special": 0},
                "2417": {"special": 0},
                "2418": {"special": 0}
            }
        },

        // Relay Station (MAP18)
        "MAP18":{
            "linedefs":{
                // Allow opening the door in the final area that closes in on you, from behind
                "2502":{"special":117}
            }
        },

        // Waste Processing (MAP19)
        "MAP19":{
            "linedefs":{
                // Allow opening the wall that traps you in with imps, from behind
                "1511":{"special":31}
            }
        },

        // Maintenance (MAP21)
        "MAP21":{
            // The hub on this level is inaccessible after you leave the opening room, but
            // because this level is trivially exitable, we don't bother to move it.
            "linedefs":{
                // Fix the exit teleporter to use W1 instead of S1
                "2187":{"special":52}
            }
        },

        // Specimen Storage (MAP22)
        "MAP22":{
            "linedefs":{
                // Remove the kinda pointless blue key self-lock.
                "706":{"special":1},

                // Lower the bookshelf blocking the danger room if approached from behind
                // (due to having the blue key earlier than intended)
                "884":{"special":19, "tag": 7}
            }
        },

        // Sub-Laboratory (MAP24)
        "MAP24":{
            // Move the hub into the room you teleport into.
            "hub":{"x":-384,"y":-128, "angle":0},

            "linedefs":{
                // Allow opening the blue key trap room from behind, if it gets shut on you.
                "638":{"special":31},

                // Allow exiting the blue key trap room if you wait it out.
                // The blue key just allows you to exit it early.
                // Done this way because the door *starts* open
                "857":{"special":86,"tag":12},
                "858":{"special":86,"tag":12},
                "859":{"special":86,"tag":12},
                "860":{"special":86,"tag":12}
            }
        },

        // Main Laboratory (MAP25)
        "MAP25":{
            "linedefs":{
                // Set up lift to let you return to the start room.
                "2790":{"special":62,"tag":4},
                "2880":{"special":62,"tag":4},
                "2881":{"special":62,"tag":4},
                "2882":{"special":62,"tag":4}
            }
        },

        // Unknown (MAP28)
        "MAP28":{
            // Move the hub so it remains reachable after leaving the starting rooms.
            "hub":{"x":-640,"y":-3232}
        },

        // Self-Destruct (MAP29)
        "MAP29":{
            "linedefs":{
                // Fix a vanilla softlock from raising platforms too fast, by making all repeatable.
                "1268":{"special":132},
                "1323":{"special":132}
            }
        },

        // Boss (MAP30)
        "MAP30":{
            "things":{
                // Angle the player start better, since we're skipping the lift.
                "0": {"angle": 135}
            },
            "sectors":{
                // Skip the lift, have it pre-lowered.
                "49": {"floor": 0}
            },
            "linedefs":{
                // Fix all teleporters to go to consistent locations.
                // We do this to quarter the total check count.
                "42":{"tag":23},
                "519":{"tag":37},
                "578":{"tag":37},
                "585":{"tag":37},
                "703":{"tag":37},
                "832":{"tag":37},
